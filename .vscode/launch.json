{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "codelldb",
      "request": "launch",
      "name": "Debug Build",
      "program": "${workspaceFolder}\\target\\debug\\ms2cc.exe",
      "args": [
        "--input-file",
        "input\\msbuild.log",
        "--output-file",
        "output\\compile_commands.json",
        "--source-directory",
        "s:\\projects\\git\\cpp"
      ],
      "cwd": "${workspaceFolder}"
    },
    {
      "type": "codelldb",
      "request": "launch",
      "name": "Release Build",
      "program": "${workspaceFolder}\\target\\release\\ms2cc.exe",
      "args": [
        "--input-file",
        "input\\msbuild.log",
        "--output-file",
        "output\\compile_commands.json",
        "--source-directory",
        "s:\\projects\\git\\cpp"
      ],
      "cwd": "${workspaceFolder}"
    }
  ]
}
